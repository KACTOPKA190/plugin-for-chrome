{   
	"manifest_version": 2,
  "name": "РазминОЧКА для глза",
	"version": "1.2",
	
	"icons": {
			"64": "/img/logo.png"
		},
    "permissions" : [
			"storage",
			"notifications" 
		],
		"options_page": "options.html",
		"web_accessible_resources": [
			"/img/logo.png"
		],
	"background" : { "scripts" : [ "/js/background.js" ], "persistent" : false },
  "browser_action": {
  		"default_title": "Досрочная разминОЧКА",
      "default_icon": "/img/logo.png",
      "default_popup": "popup.html"
	}
	
}